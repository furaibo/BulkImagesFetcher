{
    "manifest_version": 2,
    "name": "BulkImagesFetcher",
    "version": "1.0",
    "description": "Chromeで表示中のWebページ内にある画像を取得できるChrome拡張です。",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "options_page": "options.html",
    "permissions": [
        "tabs",
        "http://*/",
        "https://*/"
    ]
}
